{
	"manifest_version": 2,
	"name": "Impeach to Investigate",
	"short_name": "Impeach 45",
	"description": "Replaces the word \"Impeach\" with \"Investigate\".",
	"author": "Chad Smith",
	"version": "44.0.46",
	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": [ "content_script.js" ],
			"run_at": "document_end"
		}
	]
}
